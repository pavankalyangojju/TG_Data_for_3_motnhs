void forword() {
  analogWrite(in1, motorSpeed); // Right Motor forward Pin
  analogWrite(in2, 0);          // Right Motor backward Pin
  analogWrite(in3, motorSpeed); // Left Motor backward Pin
  analogWrite(in4, 0);          // Left Motor forward Pin
}

void backword() {
  analogWrite(in1, 0);          // Right Motor forward Pin
  analogWrite(in2, motorSpeed); // Right Motor backward Pin
  analogWrite(in3, 0);          // Left Motor backward Pin
  analogWrite(in4, motorSpeed); // Left Motor forward Pin
}

void turnRight() {
  analogWrite(in1, 0);          // Right Motor forward Pin
  analogWrite(in2, motorSpeed); // Right Motor backward Pin
  analogWrite(in3, motorSpeed); // Left Motor backward Pin
  analogWrite(in4, 0);          // Left Motor forward Pin
}

